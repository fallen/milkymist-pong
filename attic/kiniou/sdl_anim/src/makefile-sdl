#SDL Toolchain :P

AS=as
CC=gcc
LD=ld
OBJCOPY=objcopy
AR=ar
RANLIB=ranlib


#Toolchain options
MACRO=-D__SDLSDK__
INCLUDES=
ASFLAGS=$(INCLUDES)
#CFLAGS=-Wall -O3 -fomit-frame-pointer -fno-builtin -fsigned-char -fsingle-precision-constant $(INCLUDES) $(MACRO)
CFLAGS=-Wall $(INCLUDES) $(MACRO)
LDFLAGS=-lGL -lGLU `sdl-config --cflags --libs`
